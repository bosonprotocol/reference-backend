{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "es6": true,
    "commonjs": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ]
  }
}